{
  "name": "rest-bdd-testing",
  "version": "0.3.0",
  "description": "Toolchain for testing REST API, in BDD style.",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha 'test/test_*.js'",
    "cover": "nyc npm run test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "build": "babel lib --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/memlucky71/rest-bdd-testing.git"
  },
  "keywords": [
    "bdd",
    "rest",
    "api",
    "http",
    "document",
    "test",
    "testing"
  ],
  "author": "Seyyed Mohammad Borghei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/memlucky71/rest-bdd-testing/issues"
  },
  "homepage": "https://github.com/memlucky71/rest-bdd-testing#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.5.4",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "chai-subset": "^1.6.0",
    "coveralls": "^3.0.3",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "nested-object-assign": "^1.0.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "mocha": {
    "exit": true,
    "require": [
      "@babel/register"
    ]
  }
}
